{
    "name": "qobo/project-template-wordpress",
    "description": "This is a template for the new project using WordPress.",
    "license": "GPL-2.0",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "secure-http": false
    },
    "require": {
        "php": ">=5.6.0",
        "johnpbloch/wordpress-core": "4.9.1",
        "fancyguy/webroot-installer": "1.1.0",
        "johnpbloch/wordpress-core-installer": "0.2.1",
        "qobo/phake-builder": "~5.0",
        "consolidation/robo": "^1.1",
        "pyrech/composer-changelogs": "~1.4",
        "wp-cli/wp-cli": "^1.1",
        "qobo/qobo-wp-generic-theme": "1.1.2",
        "qobo/qobo-wp-brand": "2.0.1",
        "qobo/qobo-wp-shortcodes": "1.0.1",
        "wpackagist-plugin/disable-wordpress-updates": "1.6.2",
        "wpackagist-plugin/wp-theme-plugin-editor-disable": "1.0.0",
        "wpackagist-plugin/disable-comments": "1.7.1",
        "wpackagist-plugin/jetpack": "5.5.1",
        "wpackagist-plugin/custom-upload-dir": "3.4.3.1",
        "wpackagist-plugin/resize-image-after-upload": "1.8.2",
        "wpackagist-plugin/compress-png-for-wp": "1.3.5",
        "wpackagist-plugin/shortcodes-ultimate": "5.0.1",
        "wpackagist-plugin/contact-form-7": "4.9.1",
        "wpackagist-plugin/listo": "1.2",
        "wpackagist-plugin/wordpress-seo": "5.8",
        "wpackagist-plugin/wp-super-cache": "1.5.8",
        "wpackagist-plugin/google-analytics-for-wordpress": "6.2.6",
        "wpackagist-plugin/bootstrap-for-contact-form-7": "1.4.5",
        "wpackagist-plugin/multiple-post-thumbnails": "1.6.6",
        "wpackagist-plugin/wp-media-library-categories": "1.5.6",
        "wpackagist-plugin/page-links-to": "2.9.9",
        "wpackagist-plugin/akismet": "4.0.1",
        "wpackagist-plugin/uk-cookie-consent": "2.3.4",
        "phpunit/php-token-stream": "~1.4",
        "wpackagist-plugin/fast-velocity-minify": "2.2.2"
    },
    "suggest": {
        "qobo/qobo-wp-custom-admin-theme": "it fixes WP admin styles where widget input, select overlap their container",
        "wpackagist-plugin/regenerate-thumbnails": "Regenerate Thumbnails allows you to regenerate the thumbnails for your image attachments.",
        "ext-xdebug": "Allows code coverage reports and advanced debugging"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "sami/sami": "~3.2",
        "wpackagist-plugin/theme-check": "*",
        "wpackagist-plugin/debug-bar": "0.9",
        "wpackagist-plugin/debug-bar-console": "0.3",
        "wpackagist-plugin/p3-profiler": "1.5.3.9",
        "wpackagist-plugin/user-switching": "1.3.0",
        "wp-coding-standards/wpcs": "0.11.0"
    },
    "extra": {
        "wordpress-install-dir": "webroot/wp",
        "installer-paths": {
            "webroot/wp-content/plugins/{$name}": ["type:wordpress-plugin"],
            "webroot/wp-content/themes/{$name}": ["type:wordpress-theme"]
        }
    },
    "autoload": {
        "psr-4": {
            "Qobo\\Robo\\": "build/Robo",
            "Qobo\\Utility\\": "build/Utility"
        }
    }
}
