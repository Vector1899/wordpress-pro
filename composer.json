{
    "name": "qoboltd/project-template-wordpress",
    "description": "This is a template for the new project using WordPress.",
    "license": "GPL-2.0",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.4.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.4.1.zip"
                },
                "require": {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "qobo/qobo-wp-custom-theme-path",
                "type": "wordpress-plugin",
                "version": "1.0.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/QoboLtd/Qobo-WP-Custom-Theme-Path/archive/v1.0.1.zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "qobo/qobo-wp-generic-theme",
                "type": "wordpress-theme",
                "version": "1.0.5",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/QoboLtd/Qobo-WP-Generic-Theme/archive/v1.0.5.zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "qobo/qobo-wp-brand",
                "type": "wordpress-plugin",
                "version": "1.0.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/QoboLtd/Qobo-WP-Brand/archive/v1.0.1.zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "qobo/qobo-wp-shortcodes",
                "type": "wordpress-plugin",
                "version": "1.0.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/QoboLtd/Qobo-WP-Shortcodes/archive/v1.0.0.zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "wordpress": "4.4.1",
        "fancyguy/webroot-installer": "1.1.0",
        "qobo/phake-builder": "~1.0",
        "wp-cli/wp-cli": "~0.21.1",
        "qobo/qobo-wp-generic-theme": "1.0.5",
        "qobo/qobo-wp-custom-theme-path": "1.0.1",
        "qobo/qobo-wp-brand": "1.0.1",
        "qobo/qobo-wp-shortcodes": "1.0.0",
        "wpackagist-theme/twentyfifteen": "*",
        "wpackagist-plugin/disable-wordpress-updates": "1.4.2",
        "wpackagist-plugin/wp-theme-plugin-editor-disable": "1.0.0",
        "wpackagist-plugin/disable-comments": "1.3.2",
        "wpackagist-plugin/jetpack": "3.8.1",
        "wpackagist-plugin/custom-upload-dir": "3.4.3.1",
        "wpackagist-plugin/resize-image-after-upload": "1.7.2",
        "wpackagist-plugin/compress-png-for-wp": "1.3.5",
        "wpackagist-plugin/shortcodes-ultimate": "4.9.9",
        "wpackagist-plugin/contact-form-7": "4.3.1",
        "wpackagist-plugin/listo": "1.1",
        "wpackagist-plugin/wordpress-seo": "3.0.6",
        "wpackagist-plugin/wp-super-cache": "1.4.7",
        "wpackagist-plugin/google-analytics-for-wordpress": "5.4.6"
    },
    "suggest": {
        "qobo/qobo-wp-custom-admin-theme": "it fixes WP admin styles where widget input, select overlap their container",
        "wpackagist-plugin/regenerate-thumbnails": "Regenerate Thumbnails allows you to regenerate the thumbnails for your image attachments."
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "phpunit/phpunit-selenium": ">=1.2",
        "wpackagist-plugin/theme-check": "*",
        "wpackagist-plugin/debug-bar": "0.8.2",
        "wpackagist-plugin/debug-bar-console": "0.3",
        "wpackagist-plugin/p3-profiler": "1.5.3.9",
        "wpackagist-plugin/user-switching": "1.0.9"
    },
    "extra": {
        "webroot-dir": "wp",
        "webroot-package": "wordpress"
    }
